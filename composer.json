{
    "name": "68publishers/amp-demo-store",
    "description": "AMP Demo Store",
    "keywords": ["68publishers", "amp", "advertising", "demo"],
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "68publishers/amp-client": "^1.2",
        "68publishers/asset": "^3.3",
        "68publishers/environment": "^1.0",
        "68publishers/tracy-git-version": "^1.1",
        "68publishers/webpack-encore-bundle": "^3.0",
        "latte/latte": "^3.0",
        "nette/application": "^3.1",
        "nette/bootstrap": "^3.2",
        "nette/caching": "^3.2",
        "nette/component-model": "^3.0",
        "nette/di": "^3.1",
        "nette/http": "^3.2",
        "nette/neon": "^3.4",
        "nette/robot-loader": "^3.4.2",
        "nette/schema": "^1.2",
        "nette/utils": "^3.2.9",
        "tracy/tracy": "^2.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.14",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-nette": "^1.2",
        "roave/security-advisories": "dev-latest",
        "symplify/phpstan-rules": "12.0.2.72"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "68publishers/environment": true
        }
    }
}
